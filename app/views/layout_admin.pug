doctype html
html(lang=session.current_lang)
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')

    title #{title} - AVnode.net
    meta(name="robots" content="index, follow")
    link(href=canonical rel="canonical")

    link(rel='shortcut icon', href='/icons/favicon.ico', type='image/x-icon')
    link(rel='apple-touch-icon', href='/icons/apple-touch-icon.png')
    link(rel='apple-touch-icon', sizes='57x57', href='/icons/apple-touch-icon-57x57.png')
    link(rel='apple-touch-icon', sizes='72x72', href='/icons/apple-touch-icon-72x72.png')
    link(rel='apple-touch-icon', sizes='76x76', href='/icons/apple-touch-icon-76x76.png')
    link(rel='apple-touch-icon', sizes='114x114', href='/icons/apple-touch-icon-114x114.png')
    link(rel='apple-touch-icon', sizes='120x120', href='/icons/apple-touch-icon-120x120.png')
    link(rel='apple-touch-icon', sizes='144x144', href='/icons/apple-touch-icon-144x144.png')
    link(rel='apple-touch-icon', sizes='152x152', href='/icons/apple-touch-icon-152x152.png')
    link(rel='apple-touch-icon', sizes='180x180', href='/icons/apple-touch-icon-180x180.png')

    link(rel='stylesheet', type='text/css', href='/css/main.min.css')
    link(rel='stylesheet', type='text/css', href='/css/admin.min.css')
    if scripts && scripts.indexOf("video")!==-1
      link(rel='stylesheet', type='text/css', href='/css/video-js.min.css')
    link(rel='stylesheet', href='/lightgallery/css/lightgallery.min.css')
    //-link(rel='stylesheet', href='/datetimeentry/jquery.datetimeentry.css')
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" crossorigin="anonymous")
    //- link(rel="stylesheet", href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css")
    //- link(rel='stylesheet', href='/css/BsMultiSelect.min.css')
    //- link(rel='stylesheet', href='https://unpkg.com/bootstrap-table@1.16.0/dist/bootstrap-table.min.css')    
    //- link(rel='stylesheet', href='/css/admin.css')
    //- link(rel='stylesheet', href='/css/adminpro.css')
  body
    #top
    a#to_top.d-print-none(href="#top", title=global.__('Back to the top'))
      span.icon-arrow-up-circle
      span.sr-only=global.__('Back to the top')
    #app
      div
        if currentUrl.indexOf("/adminpro") == 0
          include adminpro/partials/menuHeaderAdmindev
          .container-fluid
            .row.pb-3
              .d-print-none
                include adminpro/partials/menuSuper
              .col.maincontent
                block topmenu

                include partials/flash
                .main(class=printable ? "printable" : undefined)
                  block content
                  if script
                      !=script
        else
          include admin/partials/menuHeaderAdmin
          .container-fluid
            .row.pb-3
              .d-print-none
                include admin/partials/menuSuper
              .col.maincontent
                block topmenu

                include partials/flash
                .main.mb-4
                  block content
                  if script
                      !=script


    footer
      .text-muted.text-right="AVnode.net"
    include partials/modals/modalGeneral
    include partials/modals/modalNewContent
    script(src="https://maps.googleapis.com/maps/api/js?key="+process.env.GOOGLEMAPSAPIKEY+"&libraries=places")

    script(src='/js/combo.min.js')
    if scripts && scripts.indexOf("video")!==-1
      script.
        var logo_url = "!{canonical}"
      script(src='/js/combo_video.min.js')
    script(src='/js/combo_admin.min.js')
    //- script(src="/js/adminpro.js")
    //- script(src="/js/BsMultiSelect.min.js")
    //- script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.js")
    //- script(src="/js/moment-with-locales.min.js")
    //- script(src="/js/jquery.serializejson.min.js")
    //- script(src='/js/video.min.js')
    script(src='/lightgallery/js/lightgallery.min.js')
    script(src='/lightgallery/js/lg-video.min.js')
    script(src='/datetimeentry/jquery.plugin.js')
    script(src='/datetimeentry/jquery.datetimeentry.js')
    if get
      script.
        var get = !{JSON.stringify(get)};
    if data && data.slug
      script.
        var slug = "#{data.slug}";
    script(src='/js/admin_tmp.js')
    script(src="https://cdn.jsdelivr.net/gh/xcash/bootstrap-autocomplete@v2.3.7/dist/latest/bootstrap-autocomplete.min.js")

    //- script(src="https://unpkg.com/bootstrap-table@1.16.0/dist/bootstrap-table.min.js")

    //- script(src="/js/cookielawinfo.min.js")
    script(async src="https://www.googletagmanager.com/gtag/js?id=UA-124799696-1")
    include partials/modals/cookielawinfo
    script.
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'UA-124799696-1');
    // Messenger Chat Plugin Code -->
    #fb-root
    script.
      window.fbAsyncInit = function() {
        FB.init({
          xfbml            : true,
          version          : 'v10.0'
        });
      };

      (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = 'https://connect.facebook.net/en_US/sdk/xfbml.customerchat.js';
        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));

    // Your Chat Plugin code
    .fb-customerchat(attribution="setup_tool" page_id="176966905687876")
