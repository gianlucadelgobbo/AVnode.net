extends ../layout
block content
  table.table
    thead
      tr
        td="ID"
        td=global.__("title")
        td=global.__("Production")
        td=global.__("Web")
        td=global.__("Socials")
    tbody
      if data.events.length
        each item, index in data.events
          tr
            td=index+1
            td
              a(href="/admindev/events/"+item._id)!=item.title
            td
              each user in item.users
                div
                  a(href="/"+user.slug, target="_blank")!=user.stagename
            td
              each web in item.web
                div
                  a(href=web.url, target="_blank")!=web.url
            td
              each social in item.social
                div
                  a(href=social.url, target="_blank")!=social.url
