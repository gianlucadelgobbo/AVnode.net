extends ../../layout_admin
block content
  h2=title
  hr.border.border-white
  .row
    .col-md-8.offset-md-2
      .row
        .col-md-12
          if data.events
            h2=global.__("Events")
            ul
            each item, index in data.events
              li
                a(href="/adminpro/events/"+item._id)!=item.title
