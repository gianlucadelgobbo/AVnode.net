.page-break-after-always.clearfix
  if event
    if events
      each item, index in events
        if item._id==event
          h3.float-left=item.title
    .float-right
      form.form-inline.d-print-none.text-right
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/event/"+event+"/" class=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/" ? "active" : undefined))=global.__("List")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/event/"+event+"/manage/" class=(currentUrl ==    "/adminpro/partners/"+owner+"/event/"+event+"/manage" ? "active" : undefined))=global.__("By types")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/event/"+event+"/channels/", class=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/channels/" ? "active" : undefined))=global.__("Channels")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/event/"+event+"/contacts/", class=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/contacts/" ? "active" : undefined))=global.__("Contacts")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/event/"+event+"/message/", class=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/message/" ? "active" : undefined))=global.__("Message this list")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/event/"+event+"/send/", class=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/send/" ? "active" : undefined))=global.__("Check emails queue")
        select.float-left.form-control-sm.btn-primary.mb-3(style="width:150px" onchange="if (this.value) window.location.href='/adminpro/partners/"+owner+"/event/"+event+"/'+this.value+'/'")
          option(value=0)=global.__("Grant tools")
          option(value="grantsdata" selected=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/grantsdata" ? "selected" : undefined))=global.__("Grants data")
          option(value="grantsdata_table" selected=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/grantsdata_table" ? "selected" : undefined))=global.__("Grants data table")
          option(value="grantsdata_events" selected=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/grantsdata_events" ? "selected" : undefined))=global.__("Grants events data")
          option(value="mandates" selected=(currentUrl == "/adminpro/partners/"+owner+"/event/"+event+"/mandates" ? "selected" : undefined))=global.__("Mandates")
  else
    .float-right
      form.form-inline.d-print-none.text-right
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/channels/", class=(currentUrl == "/adminpro/partners/"+owner+"/channels/" ? "active" : undefined))=global.__("Channels")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/contacts/", class=(currentUrl == "/adminpro/partners/"+owner+"/contacts/" ? "active" : undefined))=global.__("Contacts")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/message/", class=(currentUrl == "/adminpro/partners/"+owner+"/message/" ? "active" : undefined))=global.__("Message this list")
        a.btn.btn-sm.btn-primary.mb-3.mr-2(href="/adminpro/partners/"+owner+"/send/", class=(currentUrl == "/adminpro/partners/"+owner+"/send/" ? "active" : undefined))=global.__("Check emails queue")

