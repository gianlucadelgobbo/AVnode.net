.form-group
	label.labelField=__("Skype")
	#skype
		//
		- if (!data.skype || !data.skype.length) {
		-  data.skype = []
		-  data.skype.push({url:""});
		- }
		each item, i in data.skype
			.mb-3
				.input-group
					input.form-control(type='text' name='skype['+i+'][url]' placeholder=__("Skype") value=item.url)
					.input-group-append
						button.btn.btn-danger.remove_simple(type='button' data-list="skype" disabled=(data.skype.length ? "disabled" : undefined))
							i.icon-trash
	button.mb-2.btn.btn-success.btn-sm.duplicator(type='button' data-list="skype")
		i.icon-plus
		!=" "
		b=__("ADD SKYPE")

